{
  "name": "asset-transfer-basic",
  "version": "1.0.0",
  "description": "Asset Transfer Basic Application implemented in typeScript using fabric-gateway",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "pretest": "npm run lint",
    "start": "node dist/server.js"
  },
  "engineStrict": true,
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "dependencies": {
    "-": "^0.0.1",
    "@grpc/grpc-js": "~1.6.7",
    "@hyperledger/fabric-gateway": "^1.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fabric-network": "^2.2.10"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/express": "^4.17.14",
    "@types/node": "^14.18.32",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "typescript": "~4.6.4"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 80,
    "semi": false,
    "arrowParens": "avoid"
  }
}
